@model ik.Models.Ozluk_IseGirisEvrak


@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">

        @Html.ValidationSummary(true, "", new { @class = "text-danger" })

        @Html.HiddenFor(model => model.personelid)
        @Html.HiddenFor(model => model.evraktip)

        <div class="form-group">
            @Html.Label("Evrak Tanım", htmlAttributes: new { @class = "control-label col-md-3" })
            <div class="col-md-9">
                @Html.EditorFor(model => model.not, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.not, "", new { @class = "text-danger" })
            </div>
        </div>
    </div>
}


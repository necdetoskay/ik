@model IEnumerable<ik.Models.ImzaTakip>

@{
    ViewBag.Title = "PersonelImzaListe";
}

<h2>PersonelImzaListe</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Tarih)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ImzaTarih)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Aciklama)
        </th>

        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Tarih)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ImzaTarih)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Aciklama)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new {item.id}) |
                @Html.ActionLink("Details", "Details", new {item.id}) |
                @Html.ActionLink("Delete", "Delete", new {item.id})
            </td>
        </tr>
    }

</table>
@model WSY.Models.AidatGenelDurumVM

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)
    @Html.HiddenFor(model => model.DaireID)
   

    <div class="editor-label">
        @Html.LabelFor(model => model.DaireOturan)
    </div>
    <div class="editor-field">
        @Html.DisplayFor(model => model.DaireOturan)
    </div>

    
    <div class="form-group frm-mrg">
        @Html.LabelFor(m => m.AidatStr)
        @Html.TextBoxFor(m => m.AidatStr, new { @class = "form-control" })
        @Html.ValidationMessageFor(m => m.AidatStr)
    </div>
    

    <div class="form-group frm-mrg">
        @Html.LabelFor(m => m.FaizStr)
        @Html.TextBoxFor(m => m.FaizStr, new { @class = "form-control" })
        @Html.ValidationMessageFor(m => m.FaizStr)
    </div>
    
    <p>
        <input type="submit" value="Değiştir" />
    </p>
}
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")

}

